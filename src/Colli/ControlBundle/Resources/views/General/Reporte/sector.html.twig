<table>
  <thead>
    <tr>
      <th>Nombre sector</th>
      <th>Ancho</th>
      <th>Largo</th>
      <th>Canton</th>
    </tr>
  </thead>
  <tbody>
    {% for item in sectores %}
      <tr>
        <td>{{ item.descripcion }}</td>
        <td>{{ item.ancho }}  {{ item.medidaAncho }}</td>
        <td>{{ item.largo }}  {{ item.medidaLargo }}</td>
        <td>{{ item.getCanton().getDescripcion() }}</td>
      </tr>
    {% endfor%}
  </tbody>
</table>
<style>
  table, td, th {
    border: 1px solid black;
  }

  th {
    background-color: black;
    color: white;
  }
</style>